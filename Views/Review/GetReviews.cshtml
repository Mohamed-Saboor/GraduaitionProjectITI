@model IEnumerable<GraduaitionProjectITI.ViewModel.GetReviewsViewModel>

@{
    ViewData["Title"] = " Get Reviews ";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>  Product @ViewBag.ProductName  Reviews  </h1>


<table class="table">
    <thead>
        <tr>
            <th>
                Customer Name
            </th>
            <th>
                Comment
            </th>
            <th>
                Rate
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th>
                    @item.UserFullName
                </th>
                <th>
                    @item.Comment
                </th>
                <th>
                    @item.Rate  / 10    
                </th>
            </tr>
        }
    </tbody>
</table>
